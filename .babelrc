{
  "presets": [
    ["@babel/preset-env", { "targets": { "node": "current" } }],
    ["@babel/preset-react", { "runtime": "automatic" }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["babel-plugin-istanbul", {
      "exclude": [
        "**/*.cy.*",
        "**/cypress/**",
        "**/node_modules/**",
        "**/*.config.*",
        "**/pages/**",
        "**/next-env.d.ts",
        "**/demo_components/**"
      ]
    }]
  ]
} 